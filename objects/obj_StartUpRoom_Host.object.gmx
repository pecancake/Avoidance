            <string>///Difficulty Selection
if global.GAME_MORE_DIFFICULTIES_UNLOCKED
{
    if keyboard_check_pressed(vk_up){DifficultySelection+=1}
    if keyboard_check_pressed(vk_down){DifficultySelection-=1}
    
    if DifficultySelection=5{DifficultySelection=1}
    if DifficultySelection=0{DifficultySelection=4}
    
    if DifficultySelection=1{Difficulty="Normal"}
    if DifficultySelection=2{Difficulty="Hard"}
    if DifficultySelection=3{Difficulty="Insane"}
    if DifficultySelection=4{Difficulty="GOD"}
    
    if Difficulty="GOD"{DifficultyColour=merge_colour(c_white,c_yellow,GODTimer)}
    else
    {DifficultyColour=c_white}
    
    if GODTimer=0.6{FadeIn=true FadeOut=false}
    if GODTimer=1{FadeIn=false FadeOut=true}
    if FadeIn=true{GODTimer+=0.005}
    if FadeOut=true{GODTimer-=0.005}
    
    if Difficulty="Normal"{DifficultyTooltip="##1.00X Points #1.00X Damage Taken"}
    if Difficulty="Hard"{DifficultyTooltip="##2.00X Points #4.00X Damage Taken"}
    if Difficulty="Insane"{DifficultyTooltip="##3.00X Points #6.00X Damage Taken"}
    if Difficulty="GOD"{DifficultyTooltip="##10.00X Points #If you take damage #It's Game Over"}
}
