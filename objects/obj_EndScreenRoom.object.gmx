    {
        EndGameCounter+=1
        if EndGameCounter&gt;=720{Alpha-=1/240 audio_stop_sound(snd_EXP_Transfer)}
        
        draw_set_alpha(Alpha)
        
        Amount=(global.GAME_TOTAL_EXP/MaxEXP)*360
        
        x=room_width/2
        y=room_height/2
        
        draw_primitive_begin(pr_trianglefan)
        draw_vertex(x,y)
        Angle=0
        repeat(Amount)
        {
        draw_vertex(x+lengthdir_x(70,Angle),y+lengthdir_y(70,Angle))
        Angle-=1
        }
        draw_primitive_end()
        
        draw_set_alpha(1)
        CircleColour=background_colour
        draw_circle_colour(x,y,55,CircleColour,CircleColour,0)
        draw_set_alpha(Alpha)    
        
        draw_text(x,y+10,"EXP#"+string(global.GAME_TOTAL_EXP)+"/"+string(MaxEXP))
        draw_text(x,y-10,"Level: "+string(global.GAME_LEVEL))
         
        if EndGameCounter=1000{game_end()}
    }
}
else
{
