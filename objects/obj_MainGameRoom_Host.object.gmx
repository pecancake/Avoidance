            <string>///Powerups
if Powerup_Damage!="None"
{
    if Powerup_Damage="obj_Player1"{global.Player1Multiplier=DamageBuff1}
    if Powerup_Damage="obj_Player2"{global.Player2Multiplier=DamageBuff2}
    if Powerup_Damage="obj_Player3"{global.Player3Multiplier=DamageBuff3}
    if Powerup_Damage="obj_Player4"{global.Player4Multiplier=DamageBuff4}
    
    Powerup_Damage_Counter-=1
    
    if Powerup_Damage_Counter=0
    {
    if Powerup_Damage="obj_Player1"{global.Player1Multiplier-=3}
    if Powerup_Damage="obj_Player2"{global.Player2Multiplier-=3}
    if Powerup_Damage="obj_Player3"{global.Player3Multiplier-=3}
    if Powerup_Damage="obj_Player4"{global.Player4Multiplier-=3}
    Powerup_Damage="None"
    }
}
else
{
    Powerup_Damage_Counter=600
    DamageBuff1=global.Player1Multiplier+3
    DamageBuff2=global.Player2Multiplier+3
    DamageBuff3=global.Player3Multiplier+3
    DamageBuff4=global.Player4Multiplier+3
}
if Powerup_Movement!="None"
{
    if Powerup_Movement="obj_Player1"{global.Player1Speed=1.5}
    if Powerup_Movement="obj_Player2"{global.Player2Speed=1.5}
    if Powerup_Movement="obj_Player3"{global.Player3Speed=1.5}
    if Powerup_Movement="obj_Player4"{global.Player4Speed=1.5}
    
    Powerup_Movement_Counter-=1
    
    if Powerup_Movement_Counter=0
    {
    if Powerup_Movement="obj_Player1"{global.Player1Speed=1}
    if Powerup_Movement="obj_Player2"{global.Player2Speed=1}
    if Powerup_Movement="obj_Player3"{global.Player3Speed=1}
    if Powerup_Movement="obj_Player4"{global.Player4Speed=1}
    Powerup_Movement="None"
    }
}
else
{
    Powerup_Movement_Counter=600
}
if Powerup_Shield!="None"
{
    if Powerup_Shield="obj_Player1"{if !instance_exists(obj_PlayerShield){ID=instance_create(x,y,obj_PlayerShield)with(ID){Target=obj_Player1}}global.Player1Hittable=false}
    if Powerup_Shield="obj_Player2"{if !instance_exists(obj_PlayerShield){ID=instance_create(x,y,obj_PlayerShield)with(ID){Target=obj_Player2}}global.Player2Hittable=false}
